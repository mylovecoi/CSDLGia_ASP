            <div class="card-header border-0 py-5">
                <h3 class="card-title align-items-start flex-column">
                    <span class="card-label font-weight-bolder text-dark">Bảng giá đất địa bàn năm @(ThongTinHoSo?.Thoidiem.Year ?? 0)</span>
                    <span class="text-muted mt-3 font-weight-bold font-size-sm">
                        Số QĐ: @(ThongTinHoSo?.Soqd ?? "") - Thời điểm: @Helpers.ConvertDateToStr(ThongTinHoSo?.Thoidiem ?? DateTime.MinValue)
                    </span>
                </h3>
            </div>
            <!--end::Header-->
            <!--begin::Body-->
            <div class="card-body pt-0 pb-3">
                <div class="tab-content">
                    <!--begin::Table-->
                    <table class="table table-striped table-hover table-responsive-lg" id="sample_4">
                        <thead>
                            <tr class="text-center text-uppercase">
                                <th>Loại đất</th>
                                <th>Tên đường phố</th>
                                <th>Loại đường</th>
                                <th>Điểm đầu/Điểm cuối</th>
                                <th>Hệ số</th>
                                <th>VT1</th>
                                <th>VT2</th>
                                <th>VT3</th>
                                <th>VT4</th>
                                <th>VT5</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in ThongTinHoSo.GiaDatDiaBanCt.OrderBy(t=>t.Sapxep))
                            {
                                <tr>
                                    <td>@item.Loaidat</td>
                                    <td>@item.Mota</td>
                                    <td>@item.Loaiduong</td>
                                    <td>@item.Diemdau <br /> @item.Diemcuoi</td>
                                    <td style="text-align:center">@Helpers.ConvertDbToStr(item.Hesok) </td>
                                    <td style="text-align:right">@Helpers.ConvertDbToStr(item.Giavt1)</td>
                                    <td style="text-align:right">@Helpers.ConvertDbToStr(item.Giavt2)</td>
                                    <td style="text-align:right">@Helpers.ConvertDbToStr(item.Giavt3)</td>
                                    <td style="text-align:right">@Helpers.ConvertDbToStr(item.Giavt4)</td>
                                    <td style="text-align:right">@Helpers.ConvertDbToStr(item.Giavt5)</td>                                   
                                </tr>
                            }
                        </tbody>
                    </table>
                    <!--end::Table-->
                </div>
            </div>
            <!--end::Body-->
        </div>
        <!--end::Advance Table Widget 4-->
