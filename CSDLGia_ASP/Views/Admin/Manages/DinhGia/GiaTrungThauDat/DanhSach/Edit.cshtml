@model CSDLGia_ASP.Models.Manages.DinhGia.GiaDauGiaDat;
@{
    var DsDonVi = (IEnumerable<DsDonVi>)ViewData["DsDonVi"];
    //var DsDiaBan = (IEnumerable<DsDiaBan>)ViewData["DsDiaBan"];
    // var DsXaPhuong = (IEnumerable<DsXaPhuong>)ViewData["DsXaPhuong"];
    var Huyens = (IEnumerable<Districts>)ViewData["Huyens"];
    var Xas = (IEnumerable<DsXaPhuong>)ViewData["Xas"];
    var TenDiaBan = ViewData["TenDiaBan"];
    var TenXaPhuong = ViewData["TenXaPhuong"];
}

@section custom_style {
    <link rel="stylesheet" type="text/css" href="~/Admin/assets/datatables/bootstrap/dataTables.bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="~/Admin/assets/datatables/bootstrap/dataTables.css" />
}
@section custom_script {
    <script src="~/Admin/assets/js/pages/crud/forms/widgets/jquery-mask.js"></script>
    <script src="~/Admin/assets/js/pages/crud/forms/widgets/select2.js"></script>

    <script type="text/javascript" src="~/Admin/assets/datatables/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="~/Admin/assets/datatables/bootstrap/dataTables.bootstrap.js"></script>
    <script src="~/Admin/assets/datatables/scripts/table-managed.js"></script>
    <script>
        $(document).ready(function () {
            TableManaged4.init();
            MoneyDecimalMask();
        });       
    </script>
}
<div class="card card-custom">
    <div class="card-header ribbon ribbon-top ribbon-ver">
        <div class="card-title">
            <h3 class="card-label">
                Thông tin hồ sơ giá<small>&nbsp;Giá trúng thầu quyền sd đất</small><br />
            </h3>
        </div>
    </div>
    <!--begin::Form-->
    <form asp-controller="GiaTrungThauDat" asp-action="Update" method="post" enctype="multipart/form-data" id="frm_r_store">
        <div class="card-body">
            <input hidden asp-for="Madv" />
            <input hidden asp-for="Mahs" />
            <input hidden asp-for="Madiaban" />
            <div class="mb-3 font-weight-bold font-size-lg">
                <label style="color: blue; text-decoration-line: underline; font-weight: bold">Sửa thông tin hồ sơ</label>
            </div>
            @await Html.PartialAsync("~/Views/Admin/Manages/DinhGia/GiaTrungThauDat/DanhSach/Detail.cshtml")
            @await Html.PartialAsync("~/Views/Admin/Manages/DinhGia/GiaTrungThauDat/ThongTinGiayTo/Index.cshtml")
           
        </div>



        <div class="card-footer" style="text-align:center">
            <a asp-controller="GiaTrungThauDat" asp-action="Index" asp-route-Madb="@ViewData["Madb"]" class="btn btn-danger">
                <i class="la la-reply"></i>Quay lại
            </a>&ensp;
            <button type="reset" class="btn btn-secondary"><i class="la la-refresh"></i>Nhập lại</button>&ensp;
            <button type="submit" class="btn btn-primary mr-2" ><i class="la la-check"></i>Lưu Lại</button>
        </div>
    </form>
    <!--end::Form-->
</div>
@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}