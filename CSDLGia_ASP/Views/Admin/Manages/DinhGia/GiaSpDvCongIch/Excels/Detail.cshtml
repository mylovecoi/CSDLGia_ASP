@{
    int record_id = 1;
    var mahs = ViewData["Mahs"];
    var ct = (IEnumerable<GiaSpDvCongIchCt>)ViewData["modelct"];
    var spCongIchDanhmuc = (IEnumerable<GiaSpDvCongIchDm>)ViewData["GiaSpDvCongIchDm"];
}
<div class="mb-3 font-weight-bold font-size-lg">
    <label style="color: blue; text-decoration-line: underline; font-weight: bold">Thông tin hồ sơ chi tiết</label>
</div>
<div class="card card-custom" style="border: 1px solid #60aee4;">
    <div class="card-body" id="frm_data">
        <table class="table table-striped table-bordered table-hover" id="datatable_4">
            <thead>
                <tr style="text-align:center">
                    <th>Hiển Thị</th>
                    <th>Danh mục</th>
                    <th>Đơn vị tính</th>
                    <th>Mức giá từ</th>
                    <th>Mức giá đến</th>
                </tr>
               
            </thead>
            <tbody>
                @foreach (var item in ct)
                {

                    <tr>
                        <td style="text-align:center">@(record_id++)</td>
                        <td style="text-align:left">@item.HienThi</td>
                        <td style="text-align:left">@item.Ten</td>
                        <td style="text-align:left">@item.Dvt</td>
                        <td style="text-align:center">@item.Mucgiatu</td>
                        <td style="text-align:center">@item.Mucgiaden</td>      
                    </tr>
                }


            </tbody>
        </table>
        <!--end: Datatable-->
    </div>
</div>
