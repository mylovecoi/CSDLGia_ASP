@{
    int record_id = 1;
    var dsdonvi = (IEnumerable<DsDonVi>)ViewData["DsDonVi"];
    var GiaSpDvCongIchDm = (IEnumerable<GiaSpDvCongIchDm>)ViewData["GiaSpDvCongIchDm"];
}
@model IEnumerable<CSDLGia_ASP.Models.Manages.DinhGia.GiaSpDvCongIchCt>

@section custom_style {
    <link rel="stylesheet" type="text/css" href="~/Admin/assets/datatables/bootstrap/dataTables.bootstrap.css" />
    <link rel="stylesheet" type="text/css" href="~/Admin/assets/datatables/bootstrap/dataTables.css" />
}

@section custom_script {
    <script type="text/javascript" src="~/Admin/assets/datatables/js/jquery.dataTables.min.js"></script>
    <script type="text/javascript" src="~/Admin/assets/datatables/bootstrap/dataTables.bootstrap.js"></script>
    <script src="~/Admin/assets/datatables/scripts/table-managed.js"></script>
    <script>
        $(document).ready(function () {
            TableManaged.init();
        });
    </script>
}

<div class="card card-custom">
    <div class="card-header flex-wrap border-0 pt-6 pb-0">
        <div class="card-title">
            <h3 class="card-label">
                Kết quả tìm kiếm hồ sơ
            </h3>
        </div>
    </div>
    <hr />
    <div class="card-body">
        <div class="row">
            <div class="col-xl-9">
            </div>
            <div class="col-xl-3 ">
                <div class="card-toolbar text-right">
                    <a class="btn btn-primary font-weight-bolder" asp-controller="#" asp-action="#">
                        <i class="la la-print"></i>In trang
                    </a>
                </div>
            </div>
        </div>
        <table class="table table-striped table-bordered table-hover" id="sample_3">
            <thead>
                <tr>
                    <th style="text-align:center" rowspan="2">STT</th>
                    <th style="text-align:center" rowspan="2">Hiện Thị</th>
                    <th style="text-align:center" rowspan="2">Tên sản phẩm dịch vụ</th>
                    <th style="text-align:center" rowspan="2">Đơn vị tính</th>
                    <th style="text-align:center" colspan="4">Mức giá</th>
                </tr>
                <tr>
                    <th>Mức giá 1</th>
                    <th>Mức giá 2</th>
                    <th>Mức giá 3</th>
                    <th>Mức giá 4</th>
                </tr>
            </thead>
            <tbody>
                @if (Model != null)
                {
                    @foreach (var item in Model)
                    {
                        <tr style="text-align: center">
                            <td style="text-align:center">@(record_id++)</td>
                            <td style="text-align:center"> @item.Tendv</td>
                            <td style="text-align:center">@item.Soqd</td>
                            <td style="text-align:left">@item.Ten</td>
                            <td style="text-align:center">@item.Dvt</td>
                            <td style="text-align:center">@item.Mucgiatu</td>
                            <td style="text-align:center">@item.Mucgiaden</td>
                            <td style="text-align:center">@item.Mucgia3</td>
                            <td style="text-align:center">@item.Mucgia4</td>
                        </tr>
                    }
                }
            </tbody>
        </table>
    </div>
    <div class="card-footer" style="text-align:center">
        <a asp-controller="GiaSpDvCongIch" asp-action="Search" class="btn btn-danger">
            <i class="la la-reply"></i>Quay lại
        </a>
    </div>
</div>









