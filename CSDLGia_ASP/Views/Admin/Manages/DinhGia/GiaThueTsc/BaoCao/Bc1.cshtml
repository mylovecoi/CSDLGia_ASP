@{
    Layout = "_LayoutReports";
    var record_id = 1;
}
@model IEnumerable<CSDLGia_ASP.ViewModels.Manages.DinhGia.VMDinhGiaThueTsc>
<br />
<br />
<table width="96%" border="0" cellspacing="0" cellpadding="8" style="margin:0 auto 20px; text-align: center;">
    <tr>
        <td width="40%" style="vertical-align: top;">
            <hr style="width: 10%;vertical-align: top;  margin-top: 2px; font-weight: bold">
        </td>
        <td style="vertical-align: top;">
            <b>
                CỘNG HÒA XÃ HỘI CHỦ NGHĨA VIỆT NAM<br>
                Độc lập - Tự do - Hạnh phúc
            </b>
            <hr style="width: 15%;vertical-align: top; margin-top: 2px; font-weight: bold">
        </td>
    </tr>
    <tr>
        <td style="vertical-align: top;">Số: .........</td>
        <td style="text-align: right; vertical-align: top"><i style="margin-right: 25%;">, ngày....tháng....năm....</i></td>
    </tr>
</table>
<p style="text-align: center; font-weight: bold; font-size: 16px;">BÁO CÁO ĐỊNH GIÁ THUÊ TÀI SẢN CÔNG</p>

<table cellspacing="0" cellpadding="0" border="1" style="margin: 20px auto; border-collapse: collapse;font:normal 14px Times, serif;" id="data">
    <thead>
        <tr>
            <th rowspan="2" width="2%" style="text-align: center">STT</th>
            <th rowspan="2" style="text-align: center">Đơn vị thuê</th>
            <th rowspan="2" style="text-align: center">Tên tài sản</th>
            <th rowspan="2" style="text-align: center">Địa chỉ</th>
            <th colspan="2" style="text-align: center">Quyết định phê<br />duyệt Đề Án</th>
            <th colspan="2" style="text-align: center">Kêt quả <br />đấu giá</th>
            <th rowspan="2" style="text-align: center">Diện tích</th>
            <th rowspan="2" style="text-align: center">Giá dịch vụ năm</th>
            <th rowspan="2" style="text-align: center"> Hợp đồng thuê</th>
            <th rowspan="2" style="text-align: center"> Thời gian <br /> cho thuê</th>
            <th rowspan="2" style="text-align: center">Từ ngày <br /> đến ngày </th>
            <th rowspan="2" style="text-align: center"> Ghi chú</th>
        </tr>
        <tr>
            <th style="text-align: center">Số QĐ</th>
            <th style="text-align: center">Ngày tháng</th>
            <th style="text-align: center">Số QĐ</th>
            <th style="text-align: center">Ngày tháng</th>
        </tr>
    </thead>
    <tbody>
        @if (Model != null)
        {
            @foreach (var item in Model)
            {
                <tr>
                    <td style="text-align: center">@(record_id++)</td>
                    <td style="text-align: center"> @item.Dvthue </td>
                    <td style="text-align: center"> @item.Tentaisan </td>
                    <td style="text-align: center"> @item.Diachi </td>
                    <td style="text-align: center"> @item.Soqdpd </td>
                    <td style="text-align: center"> @Helpers.ConvertDateToStr(item.Thoigianpd)</td>
                    <td style="text-align: center"> @item.Soqddg </td>
                    <td style="text-align: center"> @Helpers.ConvertDateToStr(item.Thoigiandg) </td>

                    <td style="text-align: center">  </td>

                    <td style="text-align: center"> @item.Sotienthuenam </td>
                    <td style="text-align: center"> @item.Hdthue </td>
                    <td style="text-align: center"> @item.Ththue </td>
                    <td style="text-align: center"> @Helpers.ConvertDateToStr(item.Thuetungay) <br> @Helpers.ConvertDateToStr(item.Thuedenngay) </td>
                    <td style="text-align: center">  </td>
                </tr>
            }
            <tr>
                <td></td>
                <td colspan="6">Tổng cộng: @Model.Count() hồ sơ</td>
            </tr>
        }

    </tbody>
</table>
<table width="96%" border="0" cellspacing="0" cellpadding="0" style="margin: 20px auto;text-align: center; height:200px">
    <tr>
        <td width="40%" style="text-align: left; vertical-align: top;">
            <span style="font-weight: bold;font-style: italic">Nơi nhận:</span><br>
            - UBND tỉnh;<br />
            - Bộ tài chính;<br />
            - Lưu: VT, QLGCS.
        </td>
        <td style="vertical-align: top;">
            <b>THỦ TRƯỞNG ĐƠN VỊ</b><br>
            <i>(Ký tên, đóng dấu)</i>
        </td>
    </tr>
</table>